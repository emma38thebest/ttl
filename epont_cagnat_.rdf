<https://w3id.org/epont/ontology/> rdf:type owl:Ontology ;
                                    owl:versionIRI <https://w3id.org/epont/ontology/0.1/> ;
                                    owl:imports ontology1: ,
                                                <http://lawd.info/ontology/1.0/> ,
                                                ontology: ,
                                                <http://www.cidoc-crm.org/crmtex/0.1/> ,
                                                <http://www.semanticweb.org/ontologies/2015/1/EPNet-ONTOP_Ontology> ,
                                                <https://w3id.org/executionTechnique/ontology/0.1> ;
                                    dc:language :en ;
                                    dc:title "Epigraphic Ontology" ;
                                    dcterms:creator "Achille Felicetti" ,
                                                    "Francesca Murano" ,
                                                    "Frank Grieshaber" ,
                                                    "Jonathan Prag" ,
                                                    "Pietro Maria Liuzzo" ,
                                                    "Silvia Evangelisti" ;
                                    dcterms:description "This ontology merges LAWD, CRMtex, the EDH Ontology, the EPNet ontology for OBDI and the Execution Technique Ontology, to support modeling of information about inscriptions." .

#################################################################
#    Annotation properties
#################################################################

###  http://edh-www.adw.uni-heidelberg.de/edh/ontology#hasDiplomaticText
ontology1:hasDiplomaticText rdf:type owl:AnnotationProperty ;
                            rdfs:domain <http://www.cidoc-crm.org/crmtex/TX6_Transcription> .


###  http://edh-www.adw.uni-heidelberg.de/edh/ontology#hasEditionText
ontology1:hasEditionText rdf:type owl:AnnotationProperty ;
                         rdfs:domain :Edition .


###  http://edh-www.adw.uni-heidelberg.de/edh/ontology#representsTypeOfMonument
ontology1:representsTypeOfMonument rdf:type owl:AnnotationProperty ;
                                   rdfs:range ontology1:TypeOfMonument .


###  http://erlangen-crm.org/current/P43_has_dimension
